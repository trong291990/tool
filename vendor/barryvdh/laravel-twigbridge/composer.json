{
    "name": "barryvdh/laravel-twigbridge",
    "description": "Twig view integration for Laravel 4",
    "keywords": ["twig", "laravel", "twigbridge", "bridge", "views"],
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.x",
        "illuminate/view": "4.x",
        "twig/twig": "~1.12",
        "symfony/twig-bridge": ">=2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0",
        "mockery/mockery": "~0.7.2"
    },
    "autoload": {
        "psr-0": {
            "Barryvdh\\TwigBridge": "src/",
            "Barryvdh\\TwigBridge\\Tests": "tests/"
        }
    },
    "minimum-stability": "dev"
}
